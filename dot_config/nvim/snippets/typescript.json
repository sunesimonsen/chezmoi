{
  "If Statement": {
    "prefix": "if",
    "body": ["if (${1:condition}) {", "  $0", "}"],
  },
  "Else Statement": {
    "prefix": "else",
    "body": ["else {", "  $0", "}"],
  },
  "Log statement": {
    "prefix": "log",
    "body": ["console.log($0)"],
  },
  "Log variable": {
    "prefix": "logv",
    "body": ["console.log(\"$1\", $1)"],
  },
  "Function statement": {
    "prefix": "fun",
    "body": ["const $1 = ($2) => {", "  $0", "}"],
  },
  "Function": {
    "prefix": "fn",
    "body": ["($1) => {", "  $0", "}"],
  },
  "Describe block": {
    "prefix": "describe",
    "body": ["describe(\"$1\", () => {", "  $0", "})"],
  },
  "It block": {
    "prefix": "it",
    "body": ["it(\"$1\", () => {", "  $0", "})"],
  },
  "Class": {
    "prefix": "class",
    "body": ["class ${1:name} {", "  $0", "}"],
  }
}
